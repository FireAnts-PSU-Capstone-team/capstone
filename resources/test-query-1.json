{
  "table": "intake",
  "columns": [
    "dba",
    "mrl"
  ],
  "where": {
    "or": [
      {
        "column": "submission_date",
        "op": "<",
        "operand": "1/1/2015"
      },
      {
        "and": [
          {
            "column": "mrl",
            "op": "=",
            "operand": "MRL20"
          },
          {
            "column": "cash_amt",
            "op": ">",
            "operand": "$900"
          }
        ]
      }
    ]
  }
}
